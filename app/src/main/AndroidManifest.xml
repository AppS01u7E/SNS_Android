<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sns_super_android">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SNS_Super_Android">
        <activity
            android:name=".sns.timetable.TimeTableActivity"
            android:exported="true" />
        <activity
            android:name="com.example.sns.login.ui.TeacherLoginActivity"
            android:exported="true" />
        <activity
            android:name=".sns.login.ui.StartSelectActivity"
            android:exported="true" />
        <activity
            android:name=".sns.splash.SignUpDoneActivity"
            android:exported="true" />
        <activity
            android:name=".sns.notice.ui.NoticeViewActivity"
            android:exported="true" />
        <activity
            android:name=".sns.schedule.Todays_scheduleActivity"
            android:exported="true" />
        <activity
            android:name=".sns.RingTone.ui.RingToneNoticeActivity"
            android:exported="true" />
        <activity
            android:name=".sns.notice.ui.NoticeActivity"
            android:exported="true" />
        <activity
            android:name=".sns.Mypage.PasswordPopupWindowActivity"
            android:exported="true" />
        <activity
            android:name=".sns.Mypage.SettingPasswordActivity"
            android:exported="true" />
        <activity
            android:name=".sns.Mypage.SettingActivity"
            android:exported="true" />
        <activity
            android:name="com.example.sns.service.chatting.ChatActivity"
            android:exported="true" />
        <activity
            android:name=".sns.splash.IntroActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.sns.login.ui.SignUpActivity"
            android:exported="true" />
        <activity
            android:name="com.example.sns.login.ui.LoginActivity"
            android:exported="true" />
        <activity
            android:name=".sns.MainActivity"
            android:exported="true" />
    </application>

</manifest>